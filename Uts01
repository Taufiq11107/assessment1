import java.util.Scanner;

public class  Uts01 {
        public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		int a = in.nextInt();
		
		if (a%4 == 0{
		        System.out.println("Kelipatan empat");
		}
		else{
		        System.out.prinln("Bukan kelipatan empat");
		}
	}
}